{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [0],
    "arrow-body-style": [0],
    "no-unused-expressions": [0],
    "no-shadow": [0],
    "dot-notation": [0],
    "prefer-arrow-callback": [0],
    "comma-dangle": [0],
    "consistent-return": [0],
    "func-names": [0],
    "no-else-return": [0],
    "no-throw-literal": [0],
  }
}
